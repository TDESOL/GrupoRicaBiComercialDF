			{
				"name": "EXTRACT DATA COSTO DESPACHO",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "EXTRACT DATA FACT DESPACHO",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SapHanaSource",
						"query": {
							"value": "SELECT DISTINCT \n       A.matnr AS material,\n      A.bwkey AS centro,\n       CONCAT(A.lfgja, A.lfmon) AS periodo,\n       A.vprsv AS control,\n       CASE A.vprsv\n         WHEN 'V' THEN A.verpr / A.peinh\n         WHEN 'S' THEN A.stprs / A.peinh\n       END   AS costo\n\t   ,C.TASA_CAMBIARIA\nFROM   SAPHANADB.mbewh A\nLEFT JOIN (\n\n  SELECT \n    B.CD_PERIODO,\n\tC.TASA_CAMBIARIA\nFROM \n  ( SELECT DISTINCT CONCAT(LEFT(B.ANIO,4), RIGHT(B.MES,2)) as CD_PERIODO  from\nSAPHANADB.ZCDS_COSTOS B where RIGHT(B.MES,2) < '13') B left join \n(\n  SELECT  A.CD_PERIODO,\n\tLEAD(A.CD_PERIODO,1,'299901') over (ORDER BY A.CD_PERIODO) AS CD_PERIODO_FIN,\n\tA.TASA_CAMBIARIA\nFROM ( \n  SELECT \n\tLEFT(cast((99999999 - GDATU) as varchar(8)),6)  AS CD_PERIODO,\n\tAVG(UKURS) AS TASA_CAMBIARIA\nFROM SAPHANADB.TCURR A\nWHERE A.MANDT= '400'  AND FCURR = 'USD'\nGROUP BY FCURR, TCURR, LEFT(cast((99999999 - GDATU) as varchar(8)),6) ) A ) C\n\non B.CD_PERIODO >= C.CD_PERIODO AND B.CD_PERIODO < C.CD_PERIODO_FIN\n\n\n) C on C.CD_PERIODO =  CONCAT(A.lfgja, A.lfmon) \n\nWHERE  CONCAT(A.lfgja, A.lfmon) BETWEEN LEFT('@{variables('Fecha_Ini_Costo')}',6)  AND LEFT('@{variables('Fecha_Fin_Costo')}',6) \nORDER BY A.matnr, A.bwkey, CONCAT(A.lfgja, A.lfmon) ",
							"type": "Expression"
						},
						"partitionOption": "None"
					},
