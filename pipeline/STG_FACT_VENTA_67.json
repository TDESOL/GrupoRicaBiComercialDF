						"oracleReaderQuery": {
							"value": "SELECT\n    A.vbeln AS NO_FACTURA,\n    A.vgbel AS CD_DESPACHO,\n    A.AUBEL AS CD_ORDEN,\n    B.FKDAT AS FECHA,\n    A.POSNR AS DETALLE_FACTURA,\n    --'' AS VOLUMEN,\n    B.KUNRG AS CLIENTE,\n    '-1' AS CD_GERENCIA_VENTA,\n    E.bzirk AS ZONA,\n    B.BZIRK AS RUTA,\n    '' AS CD_VENDEDOR,\n    '' AS CD_ASISTENTE,\n    B.VTWEG AS CANAL,\n    --CD_TIPO_CLIENTE,\n    A.kvgr2 CD_REGION_VENTAS_CREO,\n    B.waerk AS MONEDA,\n    A.WERKS AS CENTRO,\n    B.kurrf AS TASA_CAMBIARIA,\n    A.MATNR AS MATERIAL,\n    A.FKIMG AS UNIDADES,\n    A.kzwi6 AS DESCUENTO,\n    0.00 AS NETWR,\n    A.mwsbp AS ITBIS,\n    A.netwr AS MONTO_NETO,\n    A.netwr-A.mwsbp AS MONTO_BRUTO,\n    A.pstyv AS BONIFICACION,\n    E.bzirk AS CD_VENDEDOR_SUP,\n    B.VKORG AS ORGANIZACION,\n    B.rfbsk AS CONTABILIZADO,\n    D.AUART,\n    H.PARVW,\n    I.MTART\nFROM SAPSR3.VBRP A\n    JOIN SAPSR3.VBRK B ON\n        A.VBELN=B.VBELN\n        AND B.MANDT='400' \n    JOIN SAPSR3.VBAK D ON\n        D.VBELN=A.AUBEL AND\n        D.MANDT='400'\n    JOIN SAPSR3.KNVV E ON\n        E.KUNNR=B.KUNRG \n        AND E.MANDT='400'\n    LEFT JOIN (SELECT DISTINCT MANDT, VBELN, PARVW from SAPSR3.VBPA WHERE PARVW = 'WE') H ON\n        A.VBELN = H.VBELN AND\n        A.MANDT = H.MANDT\n        AND H.MANDT='400'    \n    LEFT JOIN SAPSR3.MARA I ON\n        I.MANDT = A.MANDT AND\n        I.MATNR = A.MATNR AND\n        I.mtart IN ('ZPRT','ZMER','ZSER')\n\nWHERE B.rfbsk='C' AND A.MANDT='400' AND B.FKDAT BETWEEN  @{variables('Fecha_Ini')}  AND @{variables('Fecha_Fin')}  ",
							"type": "Expression"
						},
