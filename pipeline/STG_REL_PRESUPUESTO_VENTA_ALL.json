									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "PERIODO",
									"type": "String"
								},
								"sink": {
									"name": "CD_PERIODO_PRESUPUESTO_VENTA",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "GERENCIA",
									"type": "String"
								},
								"sink": {
									"name": "CD_GERENCIA_VENTAS",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
