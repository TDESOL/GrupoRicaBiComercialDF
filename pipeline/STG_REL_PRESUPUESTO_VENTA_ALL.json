									"name": "CD_CANAL_DISTRIBUCION",
									"type": "String"
								},
								"sink": {
									"name": "CD_CANAL_DISTRIBUCION",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "CANTIDAD_ITEMS_PRESUPUESTO",
									"type": "Decimal"
								},
								"sink": {
									"name": "CANTIDAD_ITEMS_PRESUPESTO",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 4,
									"precision": 20
								}
							},
							{
								"source": {
									"name": "MONTO_NETO_DE_VENTA_PRESUPUESTO",
									"type": "Decimal"
								},
								"sink": {
