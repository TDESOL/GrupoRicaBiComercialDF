				"typeProperties": {
					"source": {
						"type": "SapHanaSource",
						"query": "SELECT DISTINCT\n       A.MATNR AS CD_PRODUCTO,\n       UPPER(A.MAKTX) AS NOMBRE_PRODUCTO, \n       C.CD_MARCA_PROD, \n\t   LPAD(B.BISMT,18,'0') AS CD_PRODUCTO_EQV,\n\t   D.FACTOR_LITROS,\n\t   D.FACTOR_LIBRAS,\n\t   D.FACTOR_CAJAS, \n\t   D.FACTOR_PALETAS,\n\t   CASE\n\t   WHEN C.FML IN('03', '04', '70', '80', '85')\n\t\tTHEN 'LB'\n\t\tELSE 'L'\n\tEND AS UND_MED\nFROM \"SAPHANADB\".\"MAKT\" A \n\tJOIN \"SAPHANADB\".\"MARA\" B ON\n\t\tB.MATNR = A.MATNR AND\n\t\tB.MANDT = A.MANDT\n\tJOIN (SELECT DISTINCT MATERIAL, CONCAT(CONCAT(CONCAT(CONCAT(CONCAT(CONCAT(SUBSTR(A.PRDHA,2,2), '-') , SUBSTR(A.PRDHA,4,2)), '-'), SUBSTR(A.PRDHA,6,3)), '-'), SUBSTR(A.PRDHA,9,3)) AS CD_MARCA_PROD, CONCAT(CONCAT(SUBSTR(A.PRDHA,2,2), '-') , SUBSTR(A.PRDHA,4,2)) AS FML\t\n\t\t\tFROM \"SAPHANADB\".\"ZCDS_SD_INVOICE\"  A\n\t\t\tWHERE A.MANDT= '400'  AND A.PRDHA <> '' ) C ON\n\t\tC.MATERIAL = A.MATNR \n\tLEFT JOIN (SELECT MATNR, \n\t\t\t\tSUM(CASE WHEN MEINH = 'L'  THEN UMREN/UMREZ ELSE 0 END) AS FACTOR_LITROS, \n\t\t\t\tSUM(CASE WHEN MEINH = 'LB' THEN UMREN/UMREZ ELSE 0 END) AS FACTOR_LIBRAS, \n\t\t\t\tSUM(CASE WHEN MEINH = 'CS' THEN UMREN/UMREZ ELSE 0 END) AS FACTOR_CAJAS, \n\t\t\t\tSUM(CASE WHEN MEINH = 'PAL' THEN UMREN/UMREZ ELSE 0 END) AS FACTOR_PALETAS \n\t\t\t\tFROM \"SAPHANADB\".\"MARM\" A \n\t\t\t\tWHERE A.MANDT='400'  AND MEINH IN('L', 'CS', 'PAL', 'LB') \n\t\t\t\tGROUP BY MATNR\n\t\t\t\tORDER BY MATNR\n\t\t   ) D ON\n\t\tD.MATNR = A.MATNR  \nWHERE A.MANDT='400'   AND SPRAS = 'S' \nORDER BY A.MATNR",
						"partitionOption": "None"
					},
