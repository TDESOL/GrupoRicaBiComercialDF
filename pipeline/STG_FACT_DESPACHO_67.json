					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": {
							"value": "SELECT DISTINCT\n       A.matnr AS material,\n      A.bwkey AS centro,\n       CONCAT(A.lfgja, A.lfmon) AS periodo,\n       A.vprsv AS control,\n       CASE vprsv\n         WHEN 'V' THEN A.verpr / A.peinh\n         WHEN 'S' THEN A.stprs / A.peinh\n       END   AS costo\n,C.TASA_CAMBIARIA\n\nFROM   SAPSR3.mbewh A\n\nJOIN SAPSR3.LIPS B\nON B.MATNR=A.MATNR\nAND B.MANDT=A.MANDT\nAND A.bwkey=B.WERKS\n\nLEFT JOIN (SELECT\nLPAD(cast((99999999 - GDATU) as varchar(8)),6)  AS CD_PERIODO,\nAVG(UKURS) AS TASA_CAMBIARIA\nFROM SAPSR3.TCURR A\nWHERE A.MANDT= '400'  AND FCURR = 'USD'\n\nGROUP BY\nLPAD(cast((99999999 - GDATU) as varchar(8)),6)  \n) C on C.CD_PERIODO =  CONCAT(A.lfgja, A.lfmon)\n\nWHERE  CONCAT(A.lfgja, A.lfmon) BETWEEN LPAD('@{variables('Fecha_Ini')}',6)  AND LPAD('@{variables('Fecha_Fin')}',6) \nORDER BY A.matnr, A.bwkey, CONCAT(A.lfgja, A.lfmon) ",
							"type": "Expression"
						},
