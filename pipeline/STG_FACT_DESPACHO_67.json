				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": {
							"value": "SELECT \n\tA.ERDAT AS FECHA_DESPACHO,\n\tLPAD(A.ERDAT,6) AS CD_PERIODO_DESPACHO,   \n    min(C.VBELV) AS CD_ORDEN_VENTAS,\n\tA.VBELN AS CD_DESPACHO_VENTA,\n\tB.POSNR AS POS_NUM,\n\tB.KOKRS AS CD_ORG,\n\t'-1' AS CD_GERENCIA_VENTA,\n\tD.ZONA AS ZONA,\n\tD.RUTA,\n\tA.KUNNR AS CD_CLIENTE,\n\tA.WAERK AS CD_MONEDA,\n\tB.MATNR AS PRODUCTO,\n    B.WERKS AS CD_CENTRO_DISTRIBICION,\n\t0.00  AS MONTO_BONIFICACION,\n\t0.00 AS NETWR,\n\tB.VTWEG AS CD_CANAL_DISTRIBUCION,\n\tB.LGMNG AS CANT_ITEMS,  \n\t0.00 AS MONTO_NETO,\n\t0.00 AS TOTAL_IMPUESTO,\n\t0.00 AS TOTAL_DESC_ORG,\n\tCASE WHEN D.DOLARES != 0 THEN D.PESOS/D.DOLARES ELSE 0 END AS TASA_REF_MON,\n\tD.ZONA AS CD_VENDEDOR_SUP,\n\tA.spe_loekz AS INDICADOR_BORRADO \n FROM SAPSR3.LIKP A \n\tLEFT JOIN  SAPSR3.LIPS  B ON\n\t\t\tB.MANDT = A.MANDT AND\n\t\t\tB.VBELN = A.VBELN  \n\tLEFT JOIN  SAPSR3.VBFA  C ON\n\t\t\tB.VBELN = C.VBELN AND\n\t\t\tB.POSNR = C.POSNN\n\tLEFT JOIN  SAPSR3.VBFA  E ON\n\t\t\tB.VBELN = E.VBELV AND\n\t\t\tB.POSNR = E.POSNV \n    LEFT JOIN SAPSR3.ZVENTAS_GENERAL D ON\n            D.NO_FACTURA=E.VBELN and\n            D.MATERIAL = B.MATNR  \nWHERE A.MANDT = '400' AND A.ERDAT BETWEEN  @{variables('Fecha_Ini')}  AND @{variables('Fecha_Fin')}\n        and C.VBTYP_N = 'J' AND C.VBTYP_V = 'C' AND E.VBTYP_N = 'M' \ngroup by A.ERDAT , LPAD(A.ERDAT,6) , A.VBELN ,B.POSNR ,B.KOKRS ,D.ZONA ,\n\t'-1',D.RUTA ,A.KUNNR ,A.WAERK ,B.MATNR ,B.WERKS, d.bonificacion_cur, B.NETWR ,B.VTWEG ,\n\tB.LGMNG, PESOS, ITBIS ,  CASE WHEN D.DOLARES != 0 THEN D.PESOS/D.DOLARES ELSE 0 END ,D.ZONA, A.spe_loekz \nORDER BY B.MATNR , B.WERKS ,LPAD(A.ERDAT,6) ",
