					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": {
							"value": "SELECT DISTINCT\n    B.MATNR AS material,\n    B.WERKS AS centro,\n    LPAD(B.ERDAT,6) AS periodo,\n    COALESCE(A.vprsv, D.vprsv) AS control,\n    COALESCE((A.verpr / A.peinh), (D.stprs / D.peinh)) AS costo,\n    C.TASA_CAMBIARIA\nFROM SAPSR3.LIPS B  \n    LEFT JOIN SAPSR3.mbewh A ON\n        B.MATNR=A.MATNR\n        AND B.MANDT=A.MANDT\n        AND A.bwkey=B.WERKS\n        AND LPAD(B.ERDAT,6) = CONCAT(A.lfgja, A.lfmon) \n        AND A.vprsv = 'V' and A.BWTAR = ' '\n    LEFT JOIN SAPSR3.mbewh D ON\n        B.MATNR=D.MATNR\n        AND B.MANDT=D.MANDT\n        AND D.bwkey=B.WERKS\n        AND LPAD(B.ERDAT,6) = CONCAT(D.lfgja, D.lfmon)        \n        AND D.vprsv = 'S' and D.BWTAR = ' '\n    LEFT JOIN (SELECT LPAD(cast((99999999 - GDATU) as varchar(8)),6)  AS CD_PERIODO,\n                        AVG(UKURS) AS TASA_CAMBIARIA\n                FROM SAPSR3.TCURR A\n                WHERE A.MANDT= '400'  AND FCURR = 'USD'\n                GROUP BY LPAD(cast((99999999 - GDATU) as varchar(8)),6)) C on \n        C.CD_PERIODO =  LPAD(B.ERDAT,6)\n\nORDER BY B.MATNR, B.WERKS, LPAD(B.ERDAT,6)",
							"type": "Expression"
						},
